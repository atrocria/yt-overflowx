{
  "manifest_version": 3,
  "name": "YouTube No Overflow",
  "version": "1.0",
  "description": "Applies no overflow CSS to YouTube.",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "css": ["style.css"],
      "js": ["content.js"]
    }
  ]
}
